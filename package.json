{
  "name": "library-front",
  "version": "0.0.1",
  "description": "\"frontend of library web app\"",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "prepare": "husky install",
    "build": "cross-env BABEL_ENV=webpack NODE_ENV=production webpack --config webpack.prod.js --progress",
    "dev": "cross-env BABEL_ENV=webpack NODE_ENV=development webpack serve --config webpack.dev.js --progress --open",
    "lint": "yarn eslint 'src/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "graphql-generate": "graphql-codegen --config codegen.yml -r dotenv/config",
    "postgraphql-generate": "prettier --write src/app/apollo/generated/**/*.tsx"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@loadable/component": "^5.15.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "firebase": "^8.6.5",
    "graphql": "^15.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.14.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@emotion/babel-plugin": "^11.3.0",
    "@graphql-codegen/cli": "1.21.8",
    "@loadable/babel-plugin": "^5.13.2",
    "@types/loadable__component": "^5.13.3",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",

{
  "name": "library-front",
  "version": "0.0.1",
  "description": "\"frontend of library web app\"",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "build": "cross-env BABEL_ENV=webpack NODE_ENV=production webpack --config webpack.prod.js --progress --colors --display-error-details",
